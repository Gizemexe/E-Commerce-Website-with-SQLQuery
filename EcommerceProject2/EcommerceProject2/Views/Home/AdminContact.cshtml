@model EcommerceProject2.Models.Admin
@{
    ViewBag.Title = "Admin Contact";
}

<div style="display:inline-block">
    <br />
    <img src="~/Content/Images/admin.png" style="margin-left:0px" width="100" height="100" />
    <h2><b>@ViewBag.Title Informations</b></h2>
    <h3><b>Admin Name:</b> @Session["Admin_name"]</h3>
</div>
<div>
    <address style="font-size:20px">
        <b>Username-ID :</b> @Model.Admin_name -
        @Model.Admin_id


        <br />
        <abbr title="Phone"><b>P: </b>444 444 4444</abbr>

    </address>
</div>
<br />
<br />
<address>
    <strong>Support:</strong>   <a href="mailto:Support@example.com">Support@example.com</a><br />
    <strong>contact:</strong> <a href="mailto:Contact@example.com">Marketing@example.com</a>
</address>
